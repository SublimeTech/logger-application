<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping
  xmlns="urn:nhibernate-mapping-2.2"
  assembly="LoggerApi"
  namespace="LoggerApi.Models.Entities">

  <class name="Application" table="application">
    <id name="ApplicationId" column="application_id">
      <generator class="assigned" />
    </id>
    <property name="DisplayName" column="display_name"/>
    <property name="Secret" column="secret"/>
  </class>
</hibernate-mapping>